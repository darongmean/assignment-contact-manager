# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

GET     /                           controllers.ContactListController.index()
GET     /me                         controllers.ContactListController.me()

POST    /login                      controllers.ContactListController.login()
POST    /contactList                controllers.ContactListController.contactList()

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
